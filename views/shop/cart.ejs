<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if (products.length > 0) { %>
                <ul>
                    <% products.forEach(product => { %>
                        <li><%= product.productData.title %> (<%= product.qty %>)</li>
                    <% }) %>
                </ul>
            <% } else { %>
                <h1>empty Cart</h1>
            <% } %>
        </main>
        <%- include('../includes/end.ejs') %>